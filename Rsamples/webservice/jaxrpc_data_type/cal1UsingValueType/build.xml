<project name="cal1UsingValueType" default="all" basedir=".">

    <property name="is.ear.module" value="true" />

    <import file="../../../common/common-build.xml" />

    <path id="sub-components">
        <filelist dir="./cal1UsingValueType-war" files="build.xml" />
    </path>

    <target name="all" depends="build, deploy, run, runclient" />

    <property name="run.uri" value="Calculator1Service/Calculator1Service" />
    <target name="run" depends="launch" />

    <target name="runclient">
        <ant dir="./cal1UsingValueType-client" target="run" inheritall="false" />
    </target>

    <target name="clean" depends="-pre-clean, do-clean, -post-clean">
        <ant dir="./cal1UsingValueType-client" target="clean" inheritall="false" />
    </target>

</project>
