<?xml version="1.0" encoding="UTF-8"?><jms-server xmlns="http://www.tmaxsoft.com/xml/ns/jeus" xmlns:ns2="http://java.sun.com/xml/ns/javaee" xmlns:ns3="http://java.sun.com/xml/ns/persistence">
    <service-config>
        <name>jmstest</name>
        <blocking-socket>true</blocking-socket>
        <connection-timeout>1000</connection-timeout>
        <client-keepalive-timeout>5</client-keepalive-timeout>
    </service-config>
    <service-config>
        <name>channel_noconf</name>
    </service-config>
    <service-config>
        <name>channel_serverurl</name>
        <server-url>tcp://127.0.0.1:39741</server-url>
    </service-config>
    <service-config>
        <name>channel_serverport</name>
        <server-address>127.0.0.1</server-address>
        <port>39742</port>
    </service-config>
    <service-config>
        <name>channel_oneport</name>
        <server-url>oneport</server-url>
    </service-config>
    <service-config>
        <name>channel_port0</name>
        <port>0</port>
    </service-config>
    <service-config>
        <name>channel_nio</name>
        <blocking-socket>false</blocking-socket>
        <selector-count>10</selector-count>
    </service-config>
    <service-config>
        <name>channel_security</name>
        <check-security>true</check-security>
    </service-config>
    <service-config>
        <name>channel_clientlimit</name>
        <blocking-socket>false</blocking-socket>
        <client-limit>20</client-limit>
    </service-config>
    <!--service-config>
        <name>channel_ssl</name>
        <use-ssl>true</use-ssl>
        <ssl-configuration>
            <protocol>TLS</protocol>
            <key-store-type>JKS</key-store-type>
            <key-store-file>/home/michael/JEUS/6/config/keystore</key-store-file>
            <key-store-pass>1111111</key-store-pass>
            <key-management-algorithm>SunX509</key-management-algorithm>
        </ssl-configuration>
    </service-config-->
    <thread-pool>
        <min>10</min>
        <max>50</max>
    </thread-pool>
    <access-log>
        <level>FINEST</level>
    </access-log>
    <durable-subscriber>
        <client-id>Durable</client-id>
        <name>DSTest</name>
        <destination-name>Topic3</destination-name>
    </durable-subscriber>
    <connection-factory>
        <type>queue</type>
        <name>QueueCF1</name>
        <export-name>cf/QueueCF1</export-name>
    </connection-factory>
    <connection-factory>
        <type>xaqueue</type>
        <name>XAQueueCF1</name>
        <export-name>cf/XAQueueCF1</export-name>
    </connection-factory>
    <connection-factory>
        <type>topic</type>
        <name>TopicCF1</name>
        <export-name>cf/TopicCF1</export-name>
    </connection-factory>
    <connection-factory>
        <type>xatopic</type>
        <name>XATopicCF1</name>
        <export-name>cf/XATopicCF1</export-name>
    </connection-factory>

	<!-- for servicechannel-test -->
    <connection-factory>
      <type>queue</type>
      <name>cf/noconfQCF</name>
      <service>channel_noconf</service>
    </connection-factory>
    <connection-factory>
      <type>queue</type>
      <name>cf/serverurlQCF</name>
      <service>channel_serverurl</service>
    </connection-factory>
    <connection-factory>
      <type>queue</type>
      <name>cf/serverportQCF</name>
      <service>channel_serverport</service>
    </connection-factory>
    <connection-factory>
      <type>queue</type>
      <name>cf/oneportQCF</name>
      <service>channel_oneport</service>
    </connection-factory>
    <connection-factory>
      <type>queue</type>
      <name>cf/port0QCF</name>
      <service>channel_port0</service>
    </connection-factory>
    <connection-factory>
      <type>queue</type>
      <name>cf/nioQCF</name>
      <service>channel_nio</service>
    </connection-factory>
    <connection-factory>
      <type>queue</type>
      <name>cf/securityQCF</name>
      <service>channel_security</service>
    </connection-factory>
    <!--connection-factory>
      <type>queue</type>
      <name>cf/sslQCF</name>
      <service>channel_ssl</service>
    </connection-factory-->
    <connection-factory>
      <type>queue</type>
      <name>cf/clientlimitQCF</name>
      <service>channel_clientlimit</service>
    </connection-factory>

    <destination>
        <type>queue</type>
        <name>Queue1</name>
        <multiple-receiver>true</multiple-receiver>
    </destination>
    <destination>
        <type>queue</type>
        <name>Queue2</name>
        <multiple-receiver>true</multiple-receiver>
    </destination>
    <destination>
        <type>queue</type>
        <name>Queue3</name>
        <multiple-receiver>true</multiple-receiver>
        <consumer-limit>3</consumer-limit>
        <limit>512000</limit>
    </destination>
    <destination>
        <type>topic</type>
        <name>Topic1</name>
    </destination>
    <destination>
        <type>topic</type>
        <name>Topic2</name>
        <consumer-limit>3</consumer-limit>
        <limit>512000</limit>
    </destination>
    <destination>
        <type>topic</type>
        <name>Topic3</name>
    </destination>


    <!-- for ejb test -->
    <connection-factory>
        <type>queue</type>
        <name>basic/QueueCF</name>
    </connection-factory>
	<connection-factory>
        <type>xaqueue</type>
        <name>basic/XAQueueCF</name>
    </connection-factory>
    <destination>
        <type>queue</type>
        <name>queue/Example1</name>
    </destination>
    <destination>
        <type>queue</type>
        <name>queue/Example2</name>
    </destination>
    <destination>
        <type>queue</type>
        <name>queue/Example3</name>
    </destination>

    <!-- for jaxws jms transport test -->
    <connection-factory>
        <type>queue</type>
        <name>QueueConnectionFactory</name>
    </connection-factory>
    <destination>
        <type>queue</type>
        <name>ExamplesQueue</name>
        <multiple-receiver>true</multiple-receiver>
    </destination>

	<!-- journal store -->
    <persistence-store>
        <journal/>
    </persistence-store>
</jms-server>