    <context-group>
        <group-name>MyGroup</group-name>
        <webserver-connection>
            <http-listener>
                <listener-id>http1</listener-id>
                <port>9099</port>
                <thread-pool>
                    <min>30</min>
                    <max>30</max>
                    <step>1</step>
                </thread-pool>
            </http-listener>
