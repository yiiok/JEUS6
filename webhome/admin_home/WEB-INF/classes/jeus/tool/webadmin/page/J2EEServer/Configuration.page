        <binding name="deleteListener" value="listener:onDeleteUserHandler"/>
        <binding name="objectName" value="objectName"/>
        <binding name="jeusType" value="literal:UserJ2EEServer"/>
    </component>

    <component id="interopForm" type="jeus:JEUSForm">
        <binding name="title" value="message:containerc.interop.title"/>
        <!--<binding name="listener" value="listener:onInteropOk"/>-->
        <binding name="useDefaultButtons" value="false"/>
    </component>

    <component id="interop_ok" type="Submit">
        <binding name="value" value="message:JEUSForm.ok.label"/>
        <binding name="listener" value="listener:onInteropOk"/>
    </component>

    <component id="interop_cancel" type="Any">
        <binding name="type" value="literal:reset"/>
        <binding name="value" value="message:JEUSForm.cancel.label"/>
    </component>
    
    <component id="enable_interop" type="jeus:Field">
        <binding name="field" value="component:enable_interopField"/>
    </component>

    <component id="enable_interopField" type="Checkbox">
        <binding name="parentElement" value="econtainer"/>
        <binding name="elementName" value="literal:enableInterop"/>
        <binding name="value" value="enableInterop"/>
        <binding name="onclick" value="enableInteropScript"/>
        <binding name="disabled" value="!@jeus.tool.webadmin.hivemind.Lock@isEnabledEdit(getSettings(), getVisit())"/>
    </component>
    
    <component id="interop_ssl_config" type="jeus:FormSection">
        <binding name="title" value="message:containerc.interop.ssl.config.title"/>
    </component>     

    <component id="keystore_path" type="jeus:Field">
        <binding name="field" value="component:keystore_pathField"/>
    </component>

    <component id="keystore_pathField" type="jeus:JAXBTextField">
        <binding name="parentElement" value="interopSslConfig"/>
        <binding name="elementName" value="literal:keystorePath"/>
    </component>

    <component id="keystore_alias" type="jeus:Field">
        <binding name="field" value="component:keystore_aliasField"/>
    </component>

    <component id="keystore_aliasField" type="jeus:JAXBTextField">
        <binding name="parentElement" value="interopSslConfig"/>
        <binding name="elementName" value="literal:keystoreAlias"/>
    </component>    

    <component id="keystore_password_algorithmField" type="PropertySelection">
        <binding name="value" value="keystorePasswordAlgorithm"/>
