        <binding name="renderer" value="@jeus.tool.webadmin.component.render.ConfirmLinkRenderer@build('common.message.remove.confirm', jndiView.refName)"/>
    </component>

    <component id="delete" type="Image">
        <binding name="image" value="asset:delete"/>
    </component>

    <component id="jndi_infoForm" type="jeus:JEUSForm">
        <binding name="useDefaultButtons" value="false"/>
    </component>

    <component id="ref_name" type="jeus:Field">
        <binding name="field" value="component:ref_nameField"/>
        
    </component>

    <component id="ref_nameField" type="jeus:JAXBTextField">
